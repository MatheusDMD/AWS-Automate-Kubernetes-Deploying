{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "matheusmarotzke-webapp-loadbalancer"
    },
    "spec": {
        "ports": [
            {
                "name": "http",
                "port": 80,
                "targetPort": 80
            }
        ],
        "selector": {
            "app": "matheusmarotzke-webapp"
        },
        "type": "LoadBalancer"
    }
}